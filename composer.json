{
    "name": "justpark/finder-facade",
    "description": "FinderFacade is a convenience wrapper for Symfony's Finder component.",
    "homepage": "https://github.com/justpark/finder-facade",
    "license": "BSD-3-Clause",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "ext-ctype": "*",
        "theseer/fdomdocument": "^1.6",
        "symfony/finder": "^6.2.3"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
